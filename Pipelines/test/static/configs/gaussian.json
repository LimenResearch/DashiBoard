{
  "identity": {
    "type": "gaussian_encoding",
    "input": "month",
    "n_modes": 4,
    "max": 12,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "identity"
  },
  "dayofyear": {
    "type": "gaussian_encoding",
    "input": "date",
    "n_modes": 12,
    "max": 365,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "dayofyear"
  },
  "hour": {
    "type": "gaussian_encoding",
    "input": "time",
    "n_modes": 4,
    "max": 24,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "hourofday"
  },
  "minute": {
    "type": "gaussian_encoding",
    "input": "time",
    "n_modes": 1,
    "max": 24,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "minuteofhour"
  }
}
