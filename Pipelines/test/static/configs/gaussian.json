{
  "identity": {
    "type": "gaussian_encoding",
    "input": "month",
    "n_components": 4,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "identity",
    "method_options": { "max": 12 }
  },
  "dayofyear": {
    "type": "gaussian_encoding",
    "input": "date",
    "n_components": 12,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "dayofyear",
    "method_options": { "max": 365 }
  },
  "hour": {
    "type": "gaussian_encoding",
    "input": "time",
    "n_components": 4,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "hourofday",
    "method_options": { "max": 24 }
  },
  "minute": {
    "type": "gaussian_encoding",
    "input": "time",
    "n_components": 1,
    "lambda": 0.5,
    "suffix": "gaussian",
    "method": "minuteofhour",
    "method_options": { "max": 24 }
  }
}
